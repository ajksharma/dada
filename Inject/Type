    let kArrow = ./../../caterwaul/Function/Kind

in    λ(cObject : Kind)
    →     let dObject = Type
      
      in  let dArrow = ./../../caterwaul/Function/Type
      
      in    λ(f : kArrow cObject dObject)
          → λ(g : kArrow cObject dObject)
          → { inject :
                ∀(a : cObject) → dArrow { _1 = f a, _2 = g a }
            , project :
                ∀(a : cObject) → dArrow { _1 = g a, _2 = Optional (f a) }
            }
