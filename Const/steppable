  λ(object : Kind)
→ λ(arrow : object → object → Type)
→ λ(category : ./../../dhall-bhat/Category/Type object arrow)
→ λ(t : object)
→   { embed = category.identity t, project = category.identity t }
  : ./../Steppable/Type
    object
    arrow
    t
    (./../../dhall-bhat/Const/Type object object t)
