    let kArrow = ./../../caterwaul/Function/Kind

in  let kProduct = ./../../caterwaul/Tuple/Kind

in    λ(object : Kind)
    → λ(arrow : kArrow (kProduct object object) Type)
    → λ(category : ./../../caterwaul/Category/Type object arrow)
    → λ(t : object)
    →   { embed = category.identity t, project = category.identity t }
      : ./../Steppable/Type
        object
        arrow
        t
        (./../../caterwaul/Const/Type object object t)
