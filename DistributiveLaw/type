    let kArrow = ./../../caterwaul/Function/Kind

in  let kProduct = ./../../caterwaul/Tuple/Kind

in    λ(object : Kind)
    → λ(arrow : kArrow (kProduct object object) Type)
    → λ(f : kArrow object object)
    → λ(g : kArrow object object)
    → ∀(a : object) → arrow { _1 = f (g a), _2 = g (f a) }
