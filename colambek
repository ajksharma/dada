    let Functor = ./../dhall-bhat/Functor/Endo/Type

in    λ(object : Kind)
    → λ(arrow : object → object → Type)
    → λ(t : object)
    → λ(base : object → object)
    → λ(functor : Functor object arrow base)
    → λ(ana : ∀(a : object) → arrow a (base a) → arrow a t)
    → λ(project : arrow t (base t))
    → ana (base t) (functor.map t (base t) project)
