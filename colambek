    let kArrow = ./../caterwaul/Function/Kind

in  let kProduct = ./../caterwaul/Tuple/Kind

in  let Functor = ./../caterwaul/Functor/Endo/Type

in    λ(object : Kind)
    → λ(arrow : kArrow (kProduct object object) Type)
    → λ(t : object)
    → λ(base : kArrow object object)
    → λ(functor : Functor object arrow base)
    → λ ( ana
        :   ∀(a : object)
          → arrow { _1 = a, _2 = base a }
          → arrow { _1 = a, _2 = t }
        )
    → λ(project : arrow { _1 = t, _2 = base t })
    → ana (base t) (functor.map t (base t) project)
