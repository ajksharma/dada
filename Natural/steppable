    let Optional/functor =
          ./../../caterwaul/Optional/functor/endo/star
          (./../../caterwaul/Identity/Type Type)
          ./../../caterwaul/Identity/functor/monoidal

in  let embed =
            λ(fa : Optional Natural)
          → Optional/fold Natural fa Natural (λ(a : Natural) → a + 1) 0

in    { embed =
          embed
      , project =
          ./../lambek
          Type
          ./../../caterwaul/Function/Type
          Natural
          Optional
          (./recursive).cata
          embed
          Optional/functor
      }
    : ./../Steppable/Type Type ./../../caterwaul/Function/Type Natural Optional
