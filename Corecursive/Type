{- Corecursion is simply recursion in the opposite category. -}
    let kArrow = ./../../caterwaul/Function/Kind

in  let kProduct = ./../../caterwaul/Tuple/Kind

in    λ(object : Kind)
    → λ(arrow : kArrow (kProduct object object) Type)
    → ./../Recursive/Type
      object
      (./../../caterwaul/Category/Op/Type object arrow)
