  λ(object : Kind)
→ let Either =
        https://raw.githubusercontent.com/sellout/caterwaul/master/Either/Type sha256:9ac62b105c684db0a562407c0132176c20e93cb29e9434f9b600ed5806cd9fc0
  
  in    λ(f : object → Type)
      → λ(g : object → Type)
      → λ(a : object)
      → Either { _1 = f a, _2 = g a }
